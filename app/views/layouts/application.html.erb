<!DOCTYPE html>
<html>
  <head>
    <title>Skynet | Gerenciador de clientes</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if user_signed_in? %>
      <%= render 'header_signed_in' %>
    <% else %>
      <%= render 'header' %>
    <% end %>
    <%= yield %>
    <%= yield :javascripts %>
  </body>
</html>
